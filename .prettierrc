{
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": false,
  "arrowParens": "always",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "jsxBracketSameLine": false,

  "plugins": ["prettier-plugin-jinja-template"],
  "overrides": [
    {
      "files": ["*.j2", "*.jinja2"],
      "options": {
        "parser": "jinja-template"
      }
    }
  ]
}